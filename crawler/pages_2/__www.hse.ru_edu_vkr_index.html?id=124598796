$(function(){

	$('#courses-author').autocomplete({autoFocus: true, minLength: 2,
		source: function (request, response)
		{
		 $.post('/lib/ajax/edu/vkr/author', {title: request.term, __salt: Math.random() }, function (jsObj)
			{
				if (jsObj && jsObj.ok)
					response(jsObj.data);
			}, 'json');
		},
		select: function (e, ui)
		{
		 return true;
		}});


	$('#courses-title').autocomplete({autoFocus: true, minLength: 2,
		source: function (request, response)
		{
		 $.post('/lib/ajax/edu/vkr/title', {title: request.term, __salt: Math.random() }, function (jsObj)
			{
				if (jsObj && jsObj.ok)
					response(jsObj.data);
			}, 'json');
		},
		select: function (e, ui)
		{
		 return true;
		}});

	$('#courses-supervisor').autocomplete({autoFocus: true, minLength: 2,
		source: function (request, response)
		{
		 $.post('/lib/ajax/edu/vkr/supervisor', {title: request.term, __salt: Math.random() }, function (jsObj)
			{
				if (jsObj && jsObj.ok)
					response(jsObj.data);
			}, 'json');
		},
		select: function (e, ui)
		{
		 return true;
		}});
});
